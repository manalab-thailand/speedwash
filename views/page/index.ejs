            <div class="map-subtext lang" key="suvarnabhumi"></div>
            <div class="map-subtext lang" key="bangchalong"></div>
        </div>
    </div>
</a>
<!-- End Img Map -->

<!-- post -->
<div class="container-fluid max-screen">
    <div class="row">
        <div class="container-fluid">
            <div class="d-flex justify-content-between">
                <div class=""></div>
                <div class="title padding-blog lang text-uppercase" key="blog">blog</div>
                <div class="mt-auto view-btn"><a href="blog" class="view-all lang" key="viewAll">View All ></a></div>
            </div>
            <div class="col-sm-12 col-md-12 col-lg-12 col-xl-6 float-right">
                <div class="text-blogs facebookseemore Post1-seemore">
                    <%=post.data[0].message%>
                </div>
            </div>
            <div class="col-sm-12 col-md-12 col-lg-12 col-xl-6 float-left text-center padding-img-blogs">
                <img class="img-blogs " src="<%=post.data[0].attachments.data[0].media.image.src%>" alt="">
            </div>
        </div>
    </div>
    <div class="row">
        <div class="container-fluid">
            <div class="col-sm-12 col-md-12 col-lg-12 col-xl-6 float-left text-center padding-img-blogs Hide">
                <img class="img-blogs " src="<%=post.data[1].attachments.data[0].media.image.src%>" alt="">
            </div>
            <div class="col-sm-12 col-md-12 col-lg-12 col-xl-6 float-right Hide">
                <div class="text-blogs facebookseemore Post2-seemore">
                    <%=post.data[1].message%>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- End post -->

