<%- include(`../partials/header`); -%>

<!-- carousel -->
<div id="carouselExampleIndicators" class="carousel slide" data-ride="carousel">
    <ol class="carousel-indicators">
        <li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
        <li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
        <li data-target="#carouselExampleIndicators" data-slide-to="2"></li>
        <li data-target="#carouselExampleIndicators" data-slide-to="3"></li>
    </ol>
    <div class="carousel-inner">
        <div class="carousel-item active">
            <img src="images/img-carousel-1.png" class="d-block w-100 img-carousel" alt="">
        </div>
        <div class="carousel-item">
            <img src="images/img-carousel-2.png" class="d-block w-100 img-carousel" alt="">
        </div>
        <div class="carousel-item">
            <img src="images/img-carousel-3.png" class="d-block w-100 img-carousel" alt="">
        </div>
        <div class="carousel-item">
            <img src="images/img-carousel-4.png" class="d-block w-100 img-carousel" alt="">
        </div>
    </div>
    <a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev">
        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
        <span class="sr-only">Previous</span>
    </a>
    <a class="carousel-control-next" href="#carouselExampleIndicators" role="button" data-slide="next">
        <span class="carousel-control-next-icon" aria-hidden="true"></span>
        <span class="sr-only">Next</span>
    </a>
</div>
<!-- end carousel -->

<!-- WORDDD -->
<div class="container-fluid max-screen">
    <div class="row">
        <div class="col-7">
            <div class="col-12 padding-word title lang" key="speedwash">
            </div>
            <div class="col-12 subtitle lang" key="subtitlespeedwash">
            </div>
            <div class="col-12">
                <div class="text lang" key="details-speedwash">
                </div>
            </div>
        </div>
        <div class="col-5">
            <img src="images/img-washingmachine.png" class="img-washing">
        </div>
    </div>
</div>
<!-- End WORDDD -->

<!-- 4 ICON -->
<div class="container-fluid max-screen">
    <div class="row text-center padding-four-icon">
        <div class="col-3">
            <img src="images/icon-time.png" width="40%">
            <div class="col-12 title-icon lang" key="title-icon-time">
                Wash at any time
            </div>
            <div class="col-12 text-icon lang" key="icon-time">
                Need to wash your laundry at odd hours? We've got your covered.
            </div>
        </div>
        <div class="col-3">
            <img src="images/icon-detergent.png" width="40%">
            <div class="col-12 title-icon lang" key="title-icon-detergent">
                No need to detergent
            </div>
            <div class="col-12 text-icon lang" key="icon-detergent">
                Just bring your clothes and let us handle the rest.
            </div>
        </div>
        <div class="col-3">
            <img src="images/icon-30-minutes.png" width="40%">
            <div class="col-12 title-icon lang" key="title-icon-30-minutes">
                Finish your wash way faster
            </div>
            <div class="col-12 text-icon lang" key="icon-30-minutes">
                30 minutes is all you need for a full load of laundry with Speedwash.
            </div>
        </div>
        <div class="col-3">
            <img src="images/icon-study.png" width="40%">
            <div class="col-12 title-icon lang" key="title-icon-study">
                relex while washing
            </div>
            <div class="col-12 text-icon lang" key="icon-study">
                Wait for your laundry in comfort in our waiting areas.
            </div>
        </div>
    </div>
</div>
<!-- End 4 ICON -->

<!-- Img Map -->
<a href="branches">
